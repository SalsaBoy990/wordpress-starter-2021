{
    "repositories": [
      {
        "type": "composer",
        "url": "https://wpackagist.org"
      }
    ],
    "require": {
      "johnpbloch/wordpress": "^5.6.0",
      "wpackagist-plugin/regenerate-thumbnails":"^3.1.4",
      "wpackagist-plugin/akismet":"^4.1.7",
      "wpackagist-plugin/duplicator":"^1.3.40",
      "wpackagist-plugin/contact-form-7":"^5.3.2",
      "wpackagist-theme/twentynineteen":"^1.8",
      "wpackagist-theme/twentyseventeen":"^2.5",
      "wpackagist-theme/twentytwenty":"^1.6",
        "vlucas/phpdotenv": "^5.3"
    },
    "extra": {
      "wordpress-install-dir": "src/web/wp",
      "installer-paths": {
        "src/web/wp-content/plugins/{$name}/": [
          "type:wordpress-plugin"
        ],
        "src/web/wp-content/themes/{$name}/": [
          "type:wordpress-theme"
        ]
      }
    }
  }
